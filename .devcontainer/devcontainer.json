{
    "name": "GraphDB in Codespaces",
    "build": {
      "dockerfile": "Dockerfile"
    },
    "forwardPorts": [7200],
    "postCreateCommand": "docker run -d -p 7200:7200 --name graphdb-instance-name khaller/graphdb-free:10.0.0",
    "extensions": [
      "ms-azuretools.vscode-docker"
    ],
    "remoteUser": "vscode"
  }
  